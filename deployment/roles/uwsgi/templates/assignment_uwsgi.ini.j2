[uwsgi]
vhost           = true
plugins         = python
chdir           = {{project_root}}/{{project_name}}
module          = assignment.wsgi
socket          = /tmp/assignment.sock
master          = true
enable-threads  = true
processes       = 20
threads 		= 2

wsgi-file       = {{project_root}}/{{project_name}}/assignment/wsgi.py
buffer-size    = 65535
